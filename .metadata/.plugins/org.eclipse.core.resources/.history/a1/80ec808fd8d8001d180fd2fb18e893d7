package com.xworkz.Method;

public class Runner {
	public static void main(String[] args) {
		Vehicle veh = new Vehicle();
		veh.ride(138,87,1.45f);
		veh.travel();
		veh.consume("petrol");
		
		Car auto = new Car();
		auto.ride(145,87,1.20f);
		auto.travel();
		auto.consume();
		
		Bike bike = new Bike();
		auto.ride(150,87,59.45f);
		auto.travel();
		auto.consume();
				
		
		
	}

}
